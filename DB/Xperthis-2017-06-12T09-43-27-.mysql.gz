INSERT INTO `locales_source` VALUES ('7348','/fr/admin/modules?render=overlay','default','This module adds support for multilingual menus. You can setup multilingual options for each menu:','','7.54'),('7349','/fr/admin/modules?render=overlay','default','Menus can be fully multilingual with translatable (or localized) menu items.','','7.54'),('7350','/fr/admin/modules?render=overlay','default','Menus can be configured to have a fixed language. All menu items share this language setting and the menu will be visible in that language only.','','7.54'),('7351','/fr/admin/modules?render=overlay','default','Menus can also be configured to have no translations.','','7.54'),('7352','/fr/admin/modules?render=overlay','default','The multilingual options of a menu must be configured before individual menu items can be translated. Go to the <a href="@menu-admin">Menus administration page</a> and follow the "edit menu" link to the menu in question.','','7.54'),('7353','/fr/admin/modules?render=overlay','default','Supports translatable custom menu items.','','7.54'),('7354','/fr/admin/modules?render=overlay','default','Filter the modules list.','','7.54'),('7355','/fr/admin/modules?render=overlay','default','This module display a list of missing module that appear after the Drupal 7.50 release and lets you fix the entries.','','7.54'),('7356','/fr/admin/modules?render=overlay','default','Provides some extended multilingual options for nodes.','','7.54'),('7357','/fr/admin/modules?render=overlay','default','Additionally, if <em>String translation</em> enabled, this module will localize all content type configuration texts.','','7.54'),('7358','/fr/admin/modules?render=overlay','default','Content type names','','7.54'),('7359','/fr/admin/modules?render=overlay','default','Submission guidelines','','7.54'),('7360','/fr/admin/modules?render=overlay','default','Content type descriptions were previously localized so they won''t be affected.','','7.54'),('7361','/fr/admin/modules?render