DROP TABLE IF EXISTS `wpd_access_hashes`;
CREATE TABLE `wpd_access_hashes` (   `sid` int(10) unsigned NOT NULL COMMENT 'The submission identifier',   `hash` varchar(32) NOT NULL DEFAULT '' COMMENT 'Hash to identify this submission if needed',   `processed` int(10) unsigned NOT NULL COMMENT 'The timestamp when the submission has been processed',   `expires` int(10) unsigned NOT NULL COMMENT 'The timestamp when the download expires',   `used` int(10) unsigned NOT NULL COMMENT 'The timestamp when the hash has been last used',   PRIMARY KEY (`sid`) ) ENGINE=InnoDB DEFAULT CHARSET=utf8;
DROP TABLE IF EXISTS `wpd_node_configuration`;
CREATE TABLE `wpd_node_configuration` (   `nid` int(10) unsigned NOT NULL COMMENT 'The node id of the protected webform',   `mail_field_cid` int(10) unsigned NOT NULL COMMENT 'The component id of the webform that is used as mail adress to confirm the download',   `mail_from` varchar(255) DEFAULT NULL COMMENT 'The from address for the confimation mail',   `mail_subject` varchar(255) DEFAULT NULL COMMENT 'The subject for the confimation mail',   `mail_body` text COMMENT 'The body text for the confimation mail',   `text_download_access` text COMMENT 'The text for the download page, above the file listing',   `text_download_access_format` varchar(255) NOT NULL DEFAULT '' COMMENT 'The input format used for the text, above the file listing',   `text_download_noaccess` text COMMENT 'The text for the download page, if access can not be granted',   `text_download_noaccess_format` varchar(255) NOT NULL DEFAULT '' COMMENT 'The input format used for the text, if access can not be granted',   `access_type` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT 'The type of access, either 0 for one-time or 1 for repeated',   `expiration_download` int(10) unsigned NOT NULL COMMENT 'The timestamp when the download expires',   `expiration_session` int(10) unsigned NOT NULL COMMENT 'The timestamp when the session expires',   `retroactive` tinyint(3) unsigned NOT NULL DEFAULT '1' COMMENT 'The timestamp when the session expires',   `redirect` tinyint(3) unsigned NOT NULL DEFAULT '0' COMMENT 'Whether the user should be redirected to the downloads page after form submission',   PRIMARY KEY (`nid`) ) ENGINE=InnoDB DEFAULT CHARSET=utf8;
