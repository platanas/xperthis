INSERT INTO `locales_source` VALUES ('7280','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','Multi-page handling','','7.54'),('7281','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','Display all pages inside block','','7.54'),('7282','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','Redirect to the node page after the first page','','7.54'),('7283','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','If your webform has multiple pages, you may change the behavior of the "Next" button. This will also affect where validation messages show up after an error.','','7.54'),('7284','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','Display as configured in the webform','','7.54'),('7285','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','Display the confirmation page in the block on the same page (no redirect)','','7.54'),('7286','/fr/admin/structure/block/manage/webform/client-block-5/configure?render=overlay','default','This setting overrides the webform''s configuration and redirection location settings when the webform is submitted via this block.','','7.54'),('7287','/fr/admin/structure/views/settings?render=overlay','default','Show filters on the list of views','','7.54'),('7288','/fr/admin/structure/views/settings?render=overlay','default','Show advanced help warning','','7.54'),('7289','/fr/admin/structure/views/settings?render=overlay','default','Always show the master display','','7.54'),('7290','/fr/admin/structure/views/settings?render=overlay','default','Advanced users of views may choose to see the master (i.e. default) display.','','7.54'),('7291','/fr/admin/structure/views/settings?render=overlay','default','Always show advanced display settings','','7.54'),('7292','/fr/admin/structure/views/settings?r