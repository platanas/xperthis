INSERT INTO `locales_source` VALUES ('5854','','default','Account details for [user:name] at [site:name] (canceled)','','none'),('5855','','default','Cancel the selected user accounts','','none'),('5856','','default','The user account %name cannot be cancelled.','','none'),('5857','','default','When cancelling these accounts','','none'),('5858','','default','Are you sure you want to cancel these user accounts?','','none'),('5859','','default','Cancel accounts','','none'),('5860','','default','To make your password stronger:','','7.54'),('5861','','default','Make it at least 6 characters','','7.54'),('5862','','default','Add lowercase letters','','7.54'),('5863','','default','Add uppercase letters','','7.54'),('5864','','default','Add numbers','','7.54'),('5865','','default','Add punctuation','','7.54'),('5866','','default','Make it different from your username','','7.54'),('5867','','default','Weak','','7.54'),('5868','','default','Fair','','7.54'),('5869','','default','Blocked user: %name %email.','','none'),('5870','','default','There is currently 1 user online.','','none'),('5871','','default','There are currently @count users online.','','none'),('5872','','default','Sorry, there has been more than one failed login attempt for this account. It is temporarily blocked. Try again later or <a href="@url">request a new password</a>.','','none'),('5873','','default','Sorry, there have been more than @count failed login attempts for this account. It is temporarily blocked. Try again later or <a href="@url">request a new password</a>.','','none'),('5874','','default','Confirm account cancellation','','none'),('5875','','default','User passwords rehashed to improve security','','none'),('5876','','default','Migrated user time zones','','none'),('5877','','default','Minimal','','none'),('5878','','default','Start with only a few modules enabled.','','none'),('5879','','default','Basic page','','7.54'),('5880','','default','Use tags to group articles on similar topics into