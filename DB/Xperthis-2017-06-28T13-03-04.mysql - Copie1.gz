INSERT INTO `locales_target` VALUES ('4105','La langue %language a été créée.','fr','0','0','0'),('4106','Exporter une traduction','fr','0','0','0'),('4107','Sélectionnez la langue que vous voulez exporter au format gettext portable (<em>.po</em>).','fr','0','0','0'),('4108','Créer un fichier modèle gettext portable (<em>.pot</em>) contenant l''ensemble des termes de la base de données de traduction de Drupal.','fr','0','0','0'),('4109','Terme non trouvé.','fr','0','0','0'),('4110','La chaîne soumise contient des balises HTML invalides : %string','fr','0','0','0'),('4111','Le terme a été enregistré.','fr','0','0','0'),('4112','La chaîne a été supprimée.','fr','0','0','0'),('4113','La langue choisie pour l''import n''est pas prise en charge.','fr','0','0','0'),('4114','Le fichier de traduction %filename semble avoir un en-tête manquant ou malformé.','fr','0','0','0'),('4115','La traduction a été importée avec succès. Il y a %number chaîne(s) nouvellement traduite(s), %update chaîne(s) mise(s) à jour et %delete chaîne(s) supprimée(s).','fr','0','0','0'),('4116','Importation de traductions de l''interface utilisateur','fr','0','0','0'),('4117','Début de l''importation','fr','0','0','0'),('4118','Erreur à l''importation des traductions de l''interface','fr','0','0','0'),('4119','Tentative de soumission d''une traduction contenant des balises HTML invalides : %string','fr','0','0','0'),('4120','La langue %language (%code) a été créée.','fr','0','0','0'),('4121','Importation du fichier %file dans %locale : %number chaîne(s) nouvellement traduite(s), %update chaîne(s) mise(s) à jour et %delete chaîne(s) supprimée(s).','fr','0','0','0'),('4122','Fichier de traduction %locale exporté : %filename.','fr','0','0','0'),('4123','Fichier de traduction exporté : %filename.','fr','0','0','0'),('4124','Mise à jour du fichier de traduction Javascript de la langue %language.','fr','0','0','0'),('4125','Création du fichier de traduction Javascript de la langue %language.','fr','0','0','