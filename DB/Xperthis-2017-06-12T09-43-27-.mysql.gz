INSERT INTO `locales_source` VALUES ('7571','/fr/admin/appearance/settings/xperthis?render=overlay','default','This plugin is not able to be configured from the UI as it is severely broken. In an effort to balance not break backwards compatibility and to prevent new users from running into unforeseen issues, you must manually opt-in/out inside your theme .info file. Please see the following issue for more details: <a href="!url" target="_blank">Replace custom JS with the bootstrap-anchor plugin</a>','','7.54'),('7572','/fr/admin/appearance/settings/xperthis?render=overlay','default','Fix anchor positions','','7.54'),('7573','/fr/admin/appearance/settings/xperthis?render=overlay','default','Ensures anchors are correctly positioned only when there is margin or padding detected on the BODY element. This is useful when fixed navbar or administration menus are used.','','7.54'),('7574','/fr/admin/appearance/settings/xperthis?render=overlay','default','Enable smooth scrolling','','7.54'),('7575','/fr/admin/appearance/settings/xperthis?render=overlay','default','Animates page by scrolling to an anchor link target smoothly when clicked.','','7.54'),('7576','/fr/admin/appearance/settings/xperthis?render=overlay','default','Automatically remove error classes when values have been entered','','7.54'),('7577','/fr/admin/appearance/settings/xperthis?render=overlay','default','If an element has a <code>.has-error</code> class attached to it, enabling this will automatically remove that class when a value is entered. This feature compliments the "General > Forms > Make required elements display as an error" feature.','','7.54'),('7578','/fr/admin/appearance/settings/xperthis?render=overlay; sites/all/themes/bootstrap/js/bootstrap.admin.js','default','Popovers','','7.54'),('7579','/fr/admin/appearance/settings/xperthis?render=overlay','default','Add small overlays of content, like those on the iPad, to any element for housing secondary information.','','7.54'),('7580','/fr/admin/ap