INSERT INTO `locales_source` VALUES ('7221','/fr/admin/config/content/webform?render=overlay','default','HTML mail system','','7.54'),('7222','/fr/admin/config/content/webform?render=overlay','default','Whether the mail system configured for webform is capable of sending mail in HTML format.','','7.54'),('7223','/fr/admin/config/content/webform?render=overlay','default','The default format for new e-mail settings. Webform e-mail options take precedence over the settings for system-wide e-mails configured in MIME mail.','','7.54'),('7224','/fr/admin/config/content/webform?render=overlay','default','Format override','','7.54'),('7225','/fr/admin/config/content/webform?render=overlay','default','Per-webform configuration of e-mail format','','7.54'),('7226','/fr/admin/config/content/webform?render=overlay','default','Send all e-mails in the default format','','7.54'),('7227','/fr/admin/config/content/webform?render=overlay','default','Force all webform e-mails to be sent in the default format.','','7.54'),('7228','/fr/admin/config/content/webform?render=overlay','default','Track anonymous users by:','','7.54'),('7229','/fr/admin/config/content/webform?render=overlay','default','Cookie only (least strict)','','7.54'),('7230','/fr/admin/config/content/webform?render=overlay','default','IP address only','','7.54'),('7231','/fr/admin/config/content/webform?render=overlay','default','Both cookie and IP address (most strict)','','7.54'),('7232','/fr/admin/config/content/webform?render=overlay','default','<a href="http://www.wikipedia.org/wiki/HTTP_cookie">Cookies</a> can be used to help prevent the same user from repeatedly submitting a webform. Limiting by IP address is more effective against repeated submissions, but may result in unintentional blocking of users sharing the same address. Confidential submissions are tracked by cookie only. Logged-in users are always tracked by their user ID and are not affected by this option.','','7.54'),('7233','/fr/admin/config/content/