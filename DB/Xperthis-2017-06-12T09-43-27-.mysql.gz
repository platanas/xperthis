INSERT INTO `locales_source` VALUES ('9379','/fr/admin/modules/list/confirm','default','Opt-in or out of tracking','','7.54'),('9380','/fr/admin/modules/list/confirm','default','Allow users to decide if tracking code will be added to pages or not.','','7.54'),('9381','/fr/admin/modules/list/confirm','default','Use PHP for tracking visibility','','7.54'),('9382','/fr/admin/modules/list/confirm','default','Enter PHP code in the field for tracking visibility settings.','','7.54'),('9383','/fr/admin/modules/list/confirm','default','Add JavaScript snippets','','7.54'),('9384','/fr/admin/modules?filter=google&enabled=1&disabled=1&required=1&unavailable=1','default','Google Analytics','','7.54'),('9385','/fr/admin/modules?filter=google&enabled=1&disabled=1&required=1&unavailable=1','default','Configure tracking behavior to get insights into your website traffic and marketing effectiveness.','','7.54'),('9386','/fr/admin/modules?filter=google&enabled=1&disabled=1&required=1&unavailable=1','default','googleanalytics','','7.54'),('9387','/fr/admin/config/system/googleanalytics','default','General settings','','7.54'),('9388','/fr/admin/config/system/googleanalytics','default','Web Property ID','','7.54'),('9389','/fr/admin/config/system/googleanalytics','default','This ID is unique to each site you want to track separately, and is in the form of UA-xxxxxxx-yy. To get a Web Property ID, <a href="@analytics">register your site with Google Analytics</a>, or if you already have registered your site, go to your Google Analytics Settings page to see the ID next to every site profile. <a href="@webpropertyid">Find more information in the documentation</a>.','','7.54'),('9390','/fr/admin/config/system/googleanalytics','default','Tracking scope','','7.54'),('9391','/fr/admin/config/system/googleanalytics','default','Domains','','7.54'),('9392','/fr/admin/config/system/googleanalytics','default','What are you tracking?','','7.54'),('9393','/fr/admin/config/system/googleanalytics','defa