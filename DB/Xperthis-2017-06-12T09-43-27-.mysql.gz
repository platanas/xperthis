INSERT INTO `locales_source` VALUES ('7515','/fr/admin/appearance/settings/xperthis?render=overlay','default','Adds classes to buttons based on their text value. See: <a href="!bootstrap_url" target="_blank">Buttons</a> and <a href="!api_url" target="_blank">hook_bootstrap_colorize_text_alter()</a>','','7.54'),('7516','/fr/admin/appearance/settings/xperthis?render=overlay','default','Iconize Buttons','','7.54'),('7517','/fr/admin/appearance/settings/xperthis?render=overlay','default','Adds icons to buttons based on the text value. See: <a href="!api_url" target="_blank">hook_bootstrap_iconize_text_alter()</a>','','7.54'),('7518','/fr/admin/appearance/settings/xperthis?render=overlay','default','Forms','','7.54'),('7519','/fr/admin/appearance/settings/xperthis?render=overlay','default','Make required elements display as an error','','7.54'),('7520','/fr/admin/appearance/settings/xperthis?render=overlay','default','If an element in a form is required, enabling this will always display the element with a <code>.has-error</code> class. This turns the element red and helps in usability for determining which form elements are required to submit the form.  This feature compliments the "JavaScript > Forms > Automatically remove error classes when values have been entered" feature.','','7.54'),('7521','/fr/admin/appearance/settings/xperthis?render=overlay','default','Smart form descriptions (via Tooltips)','','7.54'),('7522','/fr/admin/appearance/settings/xperthis?render=overlay','default','Convert descriptions into tooltips (must be enabled) automatically based on certain criteria. This helps reduce the, sometimes unnecessary, amount of noise on a page full of form elements.','','7.54'),('7523','/fr/admin/appearance/settings/xperthis?render=overlay','default','"Smart form descriptions" maximum character limit','','7.54'),('7524','/fr/admin/appearance/settings/xperthis?render=overlay','default','Prevents descriptions from becoming tooltips by checking the character length of