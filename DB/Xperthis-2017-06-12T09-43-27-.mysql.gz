INSERT INTO `locales_source` VALUES ('7525','/fr/admin/appearance/settings/xperthis?render=overlay','default','"Smart form descriptions" allowed (HTML) tags','','7.54'),('7526','/fr/admin/appearance/settings/xperthis?render=overlay','default','Prevents descriptions from becoming tooltips by checking for HTML not in the list above (i.e. links). Separate by commas. To disable this filtering criteria, leave an empty value.','','7.54'),('7527','/fr/admin/appearance/settings/xperthis?render=overlay','default','Images','','7.54'),('7528','/fr/admin/appearance/settings/xperthis?render=overlay','default','Default image shape','','7.54'),('7529','/fr/admin/appearance/settings/xperthis?render=overlay','default','Add classes to an <code>&lt;img&gt;</code> element to easily style images in any project. Note: Internet Explorer 8 lacks support for rounded corners. See: <a href="!bootstrap_url" target="_blank">Image Shapes</a>','','7.54'),('7530','/fr/admin/appearance/settings/xperthis?render=overlay','default','Rounded','','7.54'),('7531','/fr/admin/appearance/settings/xperthis?render=overlay','default','Circle','','7.54'),('7532','/fr/admin/appearance/settings/xperthis?render=overlay','default','Thumbnail','','7.54'),('7533','/fr/admin/appearance/settings/xperthis?render=overlay; sites/all/themes/bootstrap/js/bootstrap.admin.js','default','Responsive Images','','7.54'),('7534','/fr/admin/appearance/settings/xperthis?render=overlay','default','Images in Bootstrap 3 can be made responsive-friendly via the addition of the <code>.img-responsive</code> class. This applies <code>max-width: 100%;</code> and <code>height: auto;</code> to the image so that it scales nicely to the parent element.','','7.54'),('7535','/fr/admin/appearance/settings/xperthis?render=overlay','default','Tables','','7.54'),('7536','/fr/admin/appearance/settings/xperthis?render=overlay','default','Bordered table','','7.54'),('7537','/fr/admin/appearance/settings/xperthis?render=overlay','default','Add borders on