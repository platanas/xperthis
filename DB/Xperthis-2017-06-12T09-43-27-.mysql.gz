INSERT INTO `locales_source` VALUES ('7875','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','Get from current locale (default)','','7.54'),('7876','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','Left-To-Right','','7.54'),('7877','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','Right-To-Left','','7.54'),('7878','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','Choose the language direction used in the editing area. Even when CKEditor automatically detects the user language and adjusts its user interface, the editing area is not automatically changed into the LTR or RTL mode. To be able to type LTR (like English) and RTL (like Arabic, Hebrew, Persian) content at the same time, please make sure that the <strong>!bidiltr</strong> and <strong>!bidirtl</strong> buttons are enabled in the toolbar.','','7.54'),('7879','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','Advanced content filter','','7.54'),('7880','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','this blog post','','7.54'),('7881','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','Configure input data filtering in CKEditor. See !link for more information about ACF.','','7.54'),('7882','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','<strong>Example:</strong> if only Bold and Link buttons are enabled, only !strong and !a tags will be allowed. Any !b tags will automatically be converted into !strong. The !img tag will not be allowed, unless the Image button is added to the toolbar.','','7.54'),('7883','/fr/admin/config/content/ckeditor/edit/Advanced?render=overlay','default','<strong>Warning:</strong> CKEditor will not use the set of tags associated with text format. Only the buttons enabled in the toolbar and optional plugins will determine which tags/attributes/styles are handled by CKEditor. Enabling ACF in CKEditor does not affect server-side filtering done by Drupal in any way.','','7.54');
