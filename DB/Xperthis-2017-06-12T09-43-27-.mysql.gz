INSERT INTO `locales_source` VALUES ('7170','/fr/admin/config/media/file-system?render=overlay','default','Transliterate file names during upload.','','7.54'),('7171','/fr/admin/config/media/file-system?render=overlay','default','Enable to convert file names to US-ASCII character set for cross-platform compatibility.','','7.54'),('7172','/fr/admin/config/media/file-system?render=overlay','default','Transliterate the displayed file name.','','7.54'),('7173','/fr/admin/config/media/file-system?render=overlay','default','Enable to also convert the file name that is displayed within the site (for example, in link text).','','7.54'),('7174','/fr/admin/config/media/file-system?render=overlay','default','Lowercase transliterated file names.','','7.54'),('7175','/fr/admin/config/media/file-system?render=overlay','default','This is a recommended setting to prevent issues with case-insensitive file systems. It has no effect if transliteration has been disabled.','','7.54'),('7176','/fr/admin/config/media/file-system?render=overlay&render=overlay','default','Unknown variable @name','','7.54'),('7177','/fr/admin/config/media/file-system?render=overlay&render=overlay','default','We have no meta information for this variable. Please, ask module developers to declare their variables. See <a href="http://drupal.org/project/variable">Variable module</a>.','','7.54'),('7178','/fr/node/5/protected-downloads?render=overlay','default','Access verification','','7.54'),('7179','/fr/node/5/protected-downloads?render=overlay','default','Mail confirmation to','','7.54'),('7180','/fr/node/5/protected-downloads?render=overlay','default','Webform component that will be used for the confirmation mail','','7.54'),('7181','/fr/node/5/protected-downloads?render=overlay','default','The <em>From</em> address for the confirmation mail. Leave empty to use the sites email address (%site_mail).','','7.54'),('7182','/fr/node/5/protected-downloads?render=overlay','default','Download link for [site:url]','','7.54'),('7183','/fr/node/5/protected-downloads?render=overlay','default','Dear visitor,\n\nThank you for your interest.\nPlease use the following link to download the files: [protected-downloads:download-url]\n\nThis link will be accessible until [protected-downloads:download-expires]. If you need access after the link expires, don''t hesitate to revisit the download page on [site:url]','','7.54');
INSERT INTO `locales_source` VALUES ('7184','/fr/node/5/protected-downloads?render=overlay','default','Access type','','7.54'),('7185','/fr/node/5/protected-downloads?render=overlay','default','One-time access only','','7.54'),('7186','/fr/node/5/protected-downloads?render=overlay','default','Access expires','','7.54'),('7187','/fr/node/5/protected-downloads?render=overlay','default','<em>One-time access only</em> means that the download page will only stay accessible for the duration of the session expiration period that you must configure below. Also you should update the mail body to indicate that this is a one-time access link. Note that you can safely set the downloads expiration to <em>0</em> for <em>One-time access only</em> but you need to set the session expiration to a value that is high enough that the user has time to view / download the file(s).<br /><em>Access expires</em> means that the link stays accessible until the download expires and can be accessed from different computers and persons as long as they know the link.','','7.54'),('7188','/fr/node/5/protected-downloads?render=overlay','default','Downloads expire after (seconds)','','7.54'),('7189','/fr/node/5/protected-downloads?render=overlay','default','The time in seconds that the download should be available to the user, counting from form submission. Set to 0 for no limit.','','7.54'),('7190','/fr/node/5/protected-downloads?render=overlay','default','Session expires after (seconds)','','7.54'),('7191','/fr/node/5/protected-downloads?render=overlay','default','The time in seconds that the session should be left open for a user when he has accessed the download page. The result is that the protected file stays accessible using the direct file path. Set to 0 for no limit.','','7.54'),('7192','/fr/node/5/protected-downloads?render=overlay','default','Retroactive','','7.54'),('7193','/fr/node/5/protected-downloads?render=overlay','default','Allow access to files that have been protected after the user has initially submitted the form. If unchecked, users who have received the mail with the link to the download page won''t be able to access files that have been protected after the mail has been send, unless they resubmit the form.','','7.54');
INSERT INTO `locales_source` VALUES ('7194','/fr/node/5/protected-downloads?render=overlay','default','Redirect to downloads page after form submission','','7.54'),('7195','/fr/node/5/protected-downloads?render=overlay','default','Check this, if you want to redirect the user to the downloads page, directly after the webform has been submitted. This circumvents the mail verification, thus allowing <strong>any</strong> user to access the files simply by submitting the form. Use with care!','','7.54'),('7196','/fr/node/5/protected-downloads?render=overlay','default','Protected download page','','7.54'),('7197','/fr/node/5/protected-downloads?render=overlay','default','Success','','7.54'),('7198','/fr/node/5/protected-downloads?render=overlay','default','This text will be shown above the file listing on the protected downloads page.','','7.54'),('7199','/fr/node/5/protected-downloads?render=overlay','default','Thank you for your interest. Below you will find the requested files.','','7.54'),('7200','/fr/node/5/protected-downloads?render=overlay','default','This text will be shown if the access is denied, e.g. if the download link expired.','','7.54'),('7201','/fr/node/5/protected-downloads?render=overlay','default','This link is not valid. If you think that this is an error, please contact the <a href="mailto:[site:mail]">site administrator</a>.','','7.54'),('7202','/fr/node/5/download/389ad9b7c335a292310dab43718715e8','default','No files have been found.','','7.54'),('7203','/fr/node/5/translate?render=overlay','default','Select translations for %title','','7.54'),('7204','/fr/node/5/translate?render=overlay','default','Alternatively, you can select existing nodes as translations of this one or remove nodes from this translation set. Only nodes that have the right language and don''t belong to other translation set will be available here.','','7.54'),('7205','/fr/node/5/translate?render=overlay','default','Update translations','','7.54'),('7206','/fr/admin/structure/bl